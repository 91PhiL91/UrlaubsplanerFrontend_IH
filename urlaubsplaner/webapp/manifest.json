{
  "_version": "1.49.0",
  "sap.app": {
    "id": "urlaubsplaner.urlaubsplaner",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.2",
      "toolsId": "1a8a410f-698f-48ac-90d1-ccca424fc045"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.112.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "urlaubsplaner.urlaubsplaner.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "urlaubsplaner.urlaubsplaner.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": ":?query:",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "RouteDashboard",
          "pattern": "RouteDashboard/{userID}/{token}",
          "target": [
            "TargetDashboard"
          ]
        },
        {
          "name": "RouteEmployeesManagement",
          "pattern": "RouteEmployeesManagement/{userID}/{token}",
          "target": [
            "TargetEmployeesManagement"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Login",
          "viewName": "Login"
        },
        "TargetDashboard": {
          "viewType": "XML",
          "viewId": "Dashboard",
          "viewName": "Dashboard"
        },
        "TargetEmployeesManagement": {
          "viewType": "XML",
          "viewId": "EmployeesManagement",
          "viewName": "EmployeesManagement"
        }
      }
    },
    
    "rootView": {
      "viewName": "urlaubsplaner.urlaubsplaner.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}